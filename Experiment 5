#include <stdio.h>

int main() {
    int num, i, flag = 0;
    printf("Enter Number: ");
    scanf("%d", &num);

    i = 2;
    while (i <= num / 2) {
        if (num % i == 0) {
            flag = 1;
            break;
        }
        i++;
    }

    if (flag == 0 && num > 1)
        printf("Prime\n");
    else
        printf("Not Prime\n");

    return 0;
}


#include <stdio.h>

int main() {
    int num, sum = 0, digit;
    printf("Enter a number: ");
    scanf("%d", &num);

    while (num != 0) {
        digit = num % 10;  
        sum += digit;    
        num = num / 10;    
    }

    printf("Sum of digits: %d\n", sum);
    return 0;
}


#include <stdio.h>
#include <math.h>
int main() {
    int num, original, n = 0, sum = 0, digit;
    printf("Enter a number: ");
    scanf("%d", &num);
    original = num;
    while (original != 0) {
        n++;
        original = original/10;
    }
    original = num;
    sum = 0;
    while (original != 0) {
        digit = original % 10;
        sum += (int)(round(pow(digit, n))); 
        original /= 10;
    }
    if (sum == num)
        printf("%d is an Armstrong number.\n", num);
    else
        printf("%d is not an Armstrong number.\n", num);

    return 0;}
