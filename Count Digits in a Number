#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N;
    scanf("%d", &N);
    if (N < 0) {
        N = -N;
    }
    int count = 0;
    if (N == 0) {
        count = 1;
    } else {
        while (N > 0) {
            N = N / 10;
            count++;
        }
    }
    printf("%d", count);
    return 0;
}

